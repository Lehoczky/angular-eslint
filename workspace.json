{
  "version": 2,
  "projects": {
    "builder": {
      "root": "packages/builder",
      "type": "library"
    },
    "eslint-plugin": {
      "root": "packages/eslint-plugin",
      "type": "library"
    },
    "eslint-plugin-template": {
      "root": "packages/eslint-plugin-template",
      "type": "library"
    },
    "integration-tests": {
      "root": "packages/integration-tests",
      "type": "library",
      "targets": {
        "integration-tests": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [{ "command": "lerna run integration-tests" }]
          }
        }
      }
    },
    "schematics": {
      "root": "packages/schematics",
      "type": "library"
    },
    "template-parser": {
      "root": "packages/template-parser",
      "type": "library"
    },
    "utils": {
      "root": "packages/utils",
      "type": "library"
    }
  }
}
